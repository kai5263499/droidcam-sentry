{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "globals": {
    "API_BASE": "readonly",
    "cameras": "writable",
    "recordings": "writable",
    "refreshInterval": "writable",
    "selectedRecordings": "writable",
    "bulkDeleteMode": "writable"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
